# Printing the output
def sol(dist):
    for p in range(nV):
        for q in range(nV):
            if(dist[p][q] == INF):
                print("INF", end=" ")
            else:
                print(dist[p][q], end="  ")
        print(" ")

G =     [[0     , -2    , INF   , INF   ],
         [INF   , 0     , INF   , 2     ],
         [4     , 7     , 0     , INF   ],
         [INF   , INF    ,-2    , 0     ]]
floyd(G)
